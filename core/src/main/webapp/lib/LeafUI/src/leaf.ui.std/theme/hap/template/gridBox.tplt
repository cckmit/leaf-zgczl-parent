<TABLE class="${wrapClass}" tabIndex="${tabindex}" id="${id}" hidefocus style="outline:none;${style}" cellSpacing="0" cellPadding="0" border="0">
	<TBODY>
		<TR>
			<TD width="${width}" ${title_cls} ${padding} ${colspan}><div class="gridbox-button" onclick="$le('${bindtarget}').create()"></div>${title}</TD>
		</TR>
	</TBODY>
</TABLE>
${editors}
<SCRIPT LANGUAGE="JavaScript">
	new Leaf.GridBox(${config});
	$le('${id}').bind('${bindtarget}');
</SCRIPT>