<?xml version="1.0" encoding="UTF-8"?>
<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    v.code_value,
                    v.code_value_name
                FROM
                    sys_code_values_v v
                WHERE
                    v.code IN ('HLS209_ASSET_DETAIL_IMMOVABLE_PROPERTY_M', 'HLS209_ASSET_DETAIL_MOVABLE_PROPERTY_M', 'HLS209_ASSET_DETAIL_RIGHT_P') AND
                    v.code_enabled_flag       = 'Y' AND
                    v.code_value_enabled_flag = 'Y'
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
