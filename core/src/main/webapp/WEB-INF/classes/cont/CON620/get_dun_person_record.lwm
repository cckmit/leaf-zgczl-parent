<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
                SELECT
                    e.user_id,
                    e.employee_id,
                    e.unit_id,
                    e.employee_code
                    ||'-'
                    ||e.employee_name employee_id_desc,
                    e.unit_code
                    ||'-'
                    ||e.unit_name unit_id_desc
                FROM
                    EXP_EMP_ASSIGN_E_V e
                WHERE
                    e.user_id = ${/session/@user_id}
            ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
