<bm:model xmlns:ns2="aurora.database.local.oracle" xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features" alias="t1" baseTable="HLS_PRJ_LEASE_PAYMENT_SCHEDULE">
    <bm:fields>
