<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
            select t1.* from(SELECT a.*,
       b.description business_type_name
  FROM prj_project       a,
       hls_business_type b
 WHERE a.business_type = b.business_type) t1 #WHERE_CLAUSE#
 ]]></bm:query-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
