                                                                     ${@segment_type},
                                                                     ${@segment_value},                                                     
                                                                     ${@length},    
                                                                     ${@incremental},       
                                                                     ${@start_value},                                       
                                                                     ${/session/@user_id},
                                                                     ${@date_format},
                                                                     ${@display_flag}                                                
