                <bm:parameter name="start_value" dataType="java.lang.Long"/>
            </bm:parameters>
            <bm:update-sql><![CDATA[
                 begin
                      fnd_code_rule_pkg.insert_fnd_coding_rule_details(${@coding_rule_id},
