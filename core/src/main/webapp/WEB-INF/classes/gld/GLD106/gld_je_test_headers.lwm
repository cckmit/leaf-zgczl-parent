<bm:model xmlns:bm="http://www.aurora-framework.org/schema/bm" xmlns:f="aurora.database.features">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[
    			select * from hls_journal_header
    		]]></bm:query-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
    			declare
    			v_number number;
    			begin
    				v_number:=gld_je_creation_pkg.create_je_test(p_company_id => ${/session/@company_id},
                                                p_spv_company_id => ${/session/@company_id},
                                                p_contract_id => ${@contract_id},
                                                p_accounting_date => to_date(${@lease_begin_date},'yyyy-mm-dd'),
                                                p_transaction_date => to_date(${@lease_begin_date},'yyyy-mm-dd'),
                                                p_source_code => 'A',
                                                p_source_doc_id => 1,
                                                p_je_category => 'A',
                                                p_currency_code => ${@currency_code},
                                                p_exchange_rate_type => 'A',
                                                p_exchange_rate => 1,
                                                p_attachment_num => 1,
                                                p_times => 1,
                                                p_line_quantity => 1,
                                                p_cashflow_type => 1,
                                                p_loan_contract_id => ${@contract_id},
                                                p_loan_contract_times => 1,
                                                p_csh_trx_id => 1,
                                                p_csh_write_off_id => 1,
                                                p_tenant_id => 1,
                                                p_vender_id => 1,
                                                p_lease_product_id => 1,
                                                p_product_serial_num => 1,
                                                p_user_id => ${/session/@user_id});
    			end;
    		]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
</bm:model>
