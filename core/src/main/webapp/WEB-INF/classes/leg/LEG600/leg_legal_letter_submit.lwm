<bm:model xmlns:bm="http://www.leaf-framework.org/schema/bm" needAccessControl="false">
    <bm:operations>
        <bm:operation name="query">
            <bm:query-sql><![CDATA[select  1 from dual]]></bm:query-sql>
        </bm:operation>
        <bm:operation name="update">
            <bm:update-sql><![CDATA[
               begin
                  leg_legal_pkg.legal_letter_submit(p_legal_letter_id     =>${@legal_letter_id},
							                                 p_company_id =>${/session/@company_id},
							                                 p_user_id    =>${/session/@user_id});
               end;
          ]]></bm:update-sql>
        </bm:operation>
        <bm:operation name="execute">
            <bm:update-sql><![CDATA[
               begin
                  leg_legal_pkg.create_cashflow(p_legal_letter_id     =>${@legal_letter_id},
							                    p_user_id    =>${/session/@user_id});
               end;
          ]]></bm:update-sql>
        </bm:operation>
    </bm:operations>
    <bm:fields>
        <bm:field name="1" databaseType="NUMBER" datatype="java.lang.Long" physicalName="1"/>
    </bm:fields>
</bm:model>
