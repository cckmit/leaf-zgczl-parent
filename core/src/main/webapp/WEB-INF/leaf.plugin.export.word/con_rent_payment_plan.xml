<doc>
	<header>
	</header>
	<footer />
	<body>
		<tbl border="true" indLeft="-2.5" align="center">
			<tr height="1.0">
				<tc width="8" span="1" border-left="0px">
					<p align="left">
						<t fontFamily="宋体" fontSize="22" bold="true" >附件四：</t>
					</p>
				</tc>
				<tc width="19" span="3">
					<p align="right">
						<t fontFamily="宋体" fontSize="22" bold="false" >${rec.document_type!''}</t>
						<t fontFamily="宋体" fontSize="22" bold="ture" fontColor="red"
							underline="single">${rec.contract_number!''}</t>
					</p>
				</tc>
			</tr>
			<tr>
				<tc width="27" span="4">
					<p line="400" align="center">
						<t fontFamily="宋体" fontSize="32" bold="true">租 金 支 付 表</t>
					</p>
				</tc>
			</tr>
			<tr>
				<tc width="8" span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">承租人:</t>
					</p>
				</tc>
				<tc width="19" span="3">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.bp_id_tenant_n!''}</t>
					</p>
				</tc>
			</tr>
			<tr>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">租赁物件型号及数量:</t>
					</p>
				</tc>
				<tc  width="9" span="1">
					<p align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">(详见附件三《租赁物清单》)</t>
					</p>
				</tc>
				<tc  width="4.5" span="1">
					<p align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">租赁物残值:</t>
					</p>
				</tc>
				<tc   width="5.5" span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.lease_item_salvage_value!''}</t>
					</p>
				</tc>
			</tr>
			<tr>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">租赁物购买价款:</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.lease_item_amount!''}</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">首期租金:</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.down_payment!''}</t>
					</p>
				</tc>
			</tr>
			<tr>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">租赁本金:</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.finance_amount!''}</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">租赁年利率:</t>
					</p>
				</tc>
				<tc span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.int_rate!''}</t>
					</p>
				</tc>
			</tr>
			<tr>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">租赁期限:</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.lease_start_date!''}</t>
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">至</t>
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.lease_end_date!''}</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">租赁期数:</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.lease_times!''}</t>
					</p>
				</tc>
			</tr>
			<tr>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">租金计算方法:</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.price_list_description!''}</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">支付方式:</t>
					</p>
				</tc>
				<tc  span="1">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="false" fontColor="red">${rec.pay_type!''}</t>
					</p>
				</tc>
			</tr>
		</tbl>
		<p />
		<tbl border="false" indLeft="-2.5" align="center">
			<tr >
				<tc width="27" >
					<p align="right">
						<t fontFamily="宋体" fontSize="22" bold="false">货币单位:人民币元</t>
					</p>
				</tc>
			</tr>
	    </tbl>
		<tbl  border="true" indLeft="-2.5" align="center">
			<tr>
				<tc width="1.5" span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">期次</t>
					</p>
				</tc>
				<tc width="6" span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">租金支付日(年/月/日)</t>
					</p>
				</tc>
				<tc width="4" span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">当期租金金额</t>
					</p>
				</tc>
				<tc width="3" span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">当期利率</t>
					</p>
				</tc>
				<tc width="4.5" span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">利息</t>
					</p>
				</tc>
				<tc width="4.5" span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">本金</t>
					</p>
				</tc>
				<tc width="5" span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">租赁本金余额</t>
					</p>
				</tc>
			</tr>

			<#if model.getObject('/model/payment_table').getChilds()??>
			<#list model.getObject("/model/payment_table").getChilds() as cashflow_r>
			<tr>
				<tc span="1">
					<p align="center">
						<t fontSize="20" font="宋体">${cashflow_r.getString('times')!''}</t>
					</p>
				</tc>
				<tc span="1">
					<p align="center">
						<t fontSize="20" font="宋体">${cashflow_r.getString('calc_date')!''}</t>
					</p>
				</tc>
				<tc span="1">
					<p align="center">
						<t fontSize="20" font="宋体">${cashflow_r.getString('due_amount')!''}</t>
					</p>
				</tc>
				<tc span="1">
					<p align="center">
						<t fontSize="20" font="宋体">${cashflow_r.getString('int_rate')!''}</t>
					</p>
				</tc>
				<tc span="1">
					<p align="center">
						<t fontSize="20" font="宋体">${cashflow_r.getString('interest')!''}</t>
					</p>
				</tc>
				<tc span="1">
					<p align="center">
						<t fontSize="20" font="宋体">${cashflow_r.getString('principal')!''}</t>
					</p>
				</tc>
				<tc span="1">
					<p align="center">
						<t fontSize="20" font="宋体">${cashflow_r.getString('outstanding_prin_tax_incld')!''}</t>
					</p>
				</tc>
			</tr>
			</#list>
			</#if>
			<tr>
				<tc  span="2">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">合计：</t>
					</p>
				</tc>
				<tc  span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">${rec.sum_due_amount!''}</t>
					</p>
				</tc>
				<tc span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22"></t>
					</p>
				</tc>
				<tc  span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">${rec.sum_interest!''}</t>
					</p>
				</tc>
				<tc span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22">${rec.sum_principal!''}</t>
					</p>
				</tc>
				<tc span="1">
					<p align="center">
						<t bold="true" fontFamily="宋体" fontSize="22"></t>
					</p>
				</tc>
			</tr>
			<tr>
				<tc  span="7">
					<p line="400" align="left">
						<t fontFamily="宋体" fontSize="22" bold="true">注:出租人租金收款账户信息如下:(如有变更,以出租人另行书面通知为准)</t>
					</p>
					<p line="400" align="left" indFirstLine="1.4">
						<t fontFamily="宋体" fontSize="22" bold="true">开户行:</t>
						<t fontFamily="宋体" fontSize="22" bold="true" fontColor="white">&#160;&#160; </t>
						<t fontFamily="宋体" fontSize="22" bold="true" fontColor="red">${rec.bank_branch_name!''}</t>
					</p>
					<p line="400" align="left" indFirstLine="1.4">
						<t fontFamily="宋体" fontSize="22" bold="true">户 名:</t>
						<t fontFamily="宋体" fontSize="22" bold="true" fontColor="white">&#160;&#160; </t>
						<t fontFamily="宋体" fontSize="22" bold="true" fontColor="red">${rec.bank_account_name!''}</t>
					</p>
					<p line="400" align="left" indFirstLine="1.4">
						<t fontFamily="宋体" fontSize="22" bold="true">账 号:</t>
						<t fontFamily="宋体" fontSize="22" bold="true" fontColor="white">&#160;&#160; </t>
						<t fontFamily="宋体" fontSize="22" bold="true" fontColor="red">${rec.bank_account_num!''}</t>
					</p>
				</tc>
			</tr>
		</tbl>
		<p />
		<tbl border="false">
			<tr>
				<tc width="13">
					<p line="600" align="left">
						<t fontFamily="宋体" fontSize="22" bold="ture">出租人(盖章): </t>
					</p>
				</tc>
				<tc width="13">
					<p line="600" align="center">
						<t fontFamily="宋体" fontSize="22" bold="ture">承租人(盖章): </t>
					</p>
				</tc>
			</tr>
		</tbl>
		<p />
		<p line="680" align="center">
			<t fontFamily="宋体" fontSize="22" bold="ture">签署日期: </t>
			<t fontFamily="宋体" fontSize="22" bold="ture" fontColor="red">______</t>
			<t fontFamily="宋体" fontSize="22" bold="ture">年 </t>
			<t fontFamily="宋体" fontSize="22" bold="ture" fontColor="red">____</t>
			<t fontFamily="宋体" fontSize="22" bold="ture">月 </t>
			<t fontFamily="宋体" fontSize="22" bold="ture" fontColor="red">____</t>
			<t fontFamily="宋体" fontSize="22" bold="ture">日 </t>
		</p>
		
	</body>
</doc>
