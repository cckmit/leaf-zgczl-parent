                        var downpayment_ratio = record.downpayment_ratio;
                        var arr_downpayment_ratio = $('#downpayment_ratio');
                        if (downpayment_ratio != undefined) {
                            arr_downpayment_ratio[0].value = downpayment_ratio;
                        }
            
                        var deposit = record.deposit;
