                location.href = '${/request/@context_path}/modules/touch/dashboard.screen';
            }
            
            function success(data) {
                if (data.success == true) {
                    window.location.href = '${/request/@context_path}/modules/touch/login.screen';
                }
            }
            
            function pre_step() {
                Touch.get('chance_detail_information_query').request(function(data) {
                    if (data.success) {
                        var record = data.result.record;
                        var document_type = record.document_type,
                            company_id = record.company_id;
                        
                        if (document_type != undefined && company_id != undefined) {
                            location.href = '${/request/@context_path}/modules/app/calculate2.screen?chance_id=${/parameter/@chance_id}'+'&document_type=' + document_type + '&company_id=' + company_id;
                        }
                    }
                });
            }
            var sc = new iScroll('wrapper'); //false;
            
            function callback() {
                if (!sc) {
                    sc = new iScroll('wrapper');
                } else {
                    sc.refresh();
                }
            }
            
            function next_step() {
                location.href = '${/request/@context_path}/modules/app/calculate.screen';
            }
            
            //初始化数据
            
