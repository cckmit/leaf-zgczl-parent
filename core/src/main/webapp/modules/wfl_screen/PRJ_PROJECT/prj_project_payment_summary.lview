<a:screen xmlns:a="http://www.aurora-framework.org/application">
    <a:view>
        <!-- 项目信息 -->
        <a:switch test="/parameter/@show_project_info">
            <a:case value="Y">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_payment_project_info.screen?payment_req_id=${/parameter/@payment_req_id}"/>
            </a:case>
        </a:switch>
        <!-- 资金投放 -->
        <a:switch test="/parameter/@show_payment_approval">
            <a:case value="Y">
                <a:screen-include screen="modules/wfl_screen/PRJ_PROJECT/prj_project_repayment_approval.screen?payment_req_id=${/parameter/@payment_req_id}"/>
            </a:case>
        </a:switch>
    </a:view>
</a:screen>
