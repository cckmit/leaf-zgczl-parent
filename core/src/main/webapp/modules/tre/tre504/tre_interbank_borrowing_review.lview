            <a:tabPanel width="1328">
                <a:tabs>
                    <a:tab prompt="基本信息" width="100">
                        <a:form marginWidth="30" title="借款合同信息 - 同业拆入">
                            <a:box column="4" labelWidth="120" width="1112">
                                <a:textField name="loan_contract_number" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LOAN_CONTRACT_NUMBER" readOnly="true"/>
                                <a:comboBox name="document_type_desc" bindTarget="loan_contract_ds" prompt="HLS.CONTRACT_TYPE" readOnly="true"/>
                                <a:datePicker name="loan_contract_book_date" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LOAN_CONTRACT_BOOK_DATE" readOnly="true"/>
                                <a:lov name="bank_credit_contract_number" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.BANK_CREDIT_CONTRACT_ID" readOnly="true"/>
                                <a:lov name="bank_branch_code" bindTarget="loan_contract_ds" readOnly="true"/>
                                <a:textField name="bank_branch_name" bindTarget="loan_contract_ds" readOnly="true"/>
                                <a:comboBox name="loan_certificate_flag_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LOAN_CERTIFICATE_FLAG" readOnly="true"/>
                                <a:comboBox name="related_party_flag_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.RELATED_PARTY_FLAG" readOnly="true"/>
                                <a:lov name="bank_account_code" bindTarget="loan_contract_ds" prompt="TRE502.REPAYMENT_ACCOUNT_CODE" readOnly="true">
                                    <a:events>
                                        <a:event name="focus" handler="on_bank_account_focus"/>
                                    </a:events>
                                </a:lov>
                                <a:textField name="bank_account_num" bindTarget="loan_contract_ds" prompt="TRE502.REPAYMENT_ACCOUNT_NUM" readOnly="true"/>
                                <a:lov name="withdraw_account_code" bindTarget="loan_contract_ds" prompt="TRE502.WITHDRAW_ACCOUNT_CODE" readOnly="true">
                                    <a:events>
                                        <a:event name="focus" handler="on_bank_account_focus"/>
                                    </a:events>
                                </a:lov>
                                <a:textField name="withdraw_account_num" bindTarget="loan_contract_ds" prompt="TRE502.WITHDRAW_ACCOUNT_NUM" readOnly="true"/>
                            </a:box>
                        </a:form>
                        <a:fieldSet title="借款合同信息 - 同业拆入">
                            <a:box column="4" labelWidth="120" width="1112">
                                <a:numberField name="loan_amount" allowFormat="true" allowNegative="false" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LOAN_AMOUNT_CAPITAL" readOnly="true"/>
                                <a:lov name="currency_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.CURRENCY" readOnly="true"/>
                                <a:datePicker name="loan_date_from" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LOAN_DATE_FROM" readOnly="true"/>
                                <a:datePicker name="loan_date_to" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LOAN_DATE_TO" readOnly="true"/>
                                <a:textField name="ref_v01" bindTarget="loan_contract_ds" prompt="拆借期限" readOnly="true"/>
                                <a:numberField name="consulting_fee" bindTarget="loan_contract_ds" prompt="财务顾问费"/>
                                <a:numberField name="loan_charge" bindTarget="loan_contract_ds" prompt="手续费"/>
                                <a:numberField name="other_fee" bindTarget="loan_contract_ds" prompt="其他费用"/>
                            </a:box>
                        </a:fieldSet>
                        <a:fieldSet title="TRE502.FIELDSET.LOAN_INTEREST">
                            <a:box column="4" labelWidth="120" width="1112">
                                <a:percentField name="base_rate" allowNegative="false" bindTarget="loan_contract_ds" prompt="HLS.BASE_RATE" readOnly="true"/>
                                <a:comboBox name="int_rate_fixing_way_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.INT_RATE_FIXING_WAY" readOnly="true"/>
                                <a:percentField name="int_rate_fixing_range" allowDecimals="true" allowNegative="true" bindTarget="loan_contract_ds" prompt="TRE503.INT_RATE_FIXING_RANGE" readOnly="true"/>
                                <a:percentField name="interest_rate" allowNegative="false" bindTarget="loan_contract_ds" prompt="TRE503.INTEREST_RATE" readOnly="true"/>
                                <a:comboBox name="calc_method_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.CALC_METHOD" readOnly="true"/>
                                <a:comboBox name="interest_period_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.INTEREST_PERIOD" readOnly="true"/>
                                <a:numberField name="interest_calc_date" allowDecimals="false" allowNegative="false" bindTarget="loan_contract_ds" max="31" min="1" prompt="TRE_LOAN_CONTRACT.INTEREST_CALC_DATE" readOnly="true"/>
                                <a:numberField name="interest_payment_date" allowDecimals="false" allowNegative="false" bindTarget="loan_contract_ds" max="31" min="1" prompt="TRE_LOAN_CONTRACT.INTEREST_PAYMENT_DATE" readOnly="true"/>
                                <a:percentField name="penalty_rate" bindTarget="loan_contract_ds" prompt="TRE503.PENALTY_RATE" readOnly="true"/>
                                <a:percentField name="misappropriate_rate" bindTarget="loan_contract_ds" prompt="TRE503.MISAPPROPRIATE_RATE" readOnly="true"/>
                                <a:percentField name="et_penalty_rate" bindTarget="loan_contract_ds" prompt="TRE503.ET_PENALTY_RATE" readOnly="true"/>
                                <a:numberField name="annual_days" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.ANNUAL_DAYS" readOnly="true"/>
                                <a:comboBox name="rate_float_cycle_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.RATE_FLOAT_CYCLE" readOnly="true"/>
                                <a:comboBox name="flt_execute_times_day_desc" id="cb_flt_execute_times_day" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.FLT_EXECUTE_TIMES_DAY" readOnly="true"/>
                                <a:comboBox name="adjust_day_desc" id="cb_adjust_day" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.ADJUST_DAY" readOnly="true"/>
                            </a:box>
                        </a:fieldSet>
                        <a:fieldSet column="1" title="TRE502.FIELDSET.WITHDRAW_METHOD">
                            <a:box column="1" labelWidth="120" width="280">
                                <a:comboBox name="withdrawal_method_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.WITHDRAWAL_METHOD" readOnly="true"/>
                            </a:box>
                            <div id="WC_finalWithdraw" style="display:none;">
                                <a:box column="1" labelWidth="120" width="280">
                                    <a:datePicker name="last_withdrawal_date" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LAST_WITHDRAWAL_DATE" readOnly="true"/>
                                </a:box>
                            </div>
                            <div id="WC_planWithdraw" style="display:none;">
                                <a:box>
                                    <a:box style="margin-left:116px;">
                                        <a:fieldSet title="TRE502.FIELDSET.WITHDRAWAL_SCHEDULE">
                                            <a:grid bindTarget="loan_con_withdrawal_plan_ds" height="160" navBar="true" width="350">
                                                <a:columns>
                                                    <a:column name="widthdrawal_date" prompt="TRE_LOAN_CON_WITHDRAWAL_PLAN.WIDTHDRAWAL_DATE" renderer="Aurora.formatDate"/>
                                                    <a:column name="amount" align="right" prompt="TRE_LOAN_CON_WITHDRAWAL_PLAN.AMOUNT" renderer="Aurora.formatMoney"/>
                                                    <a:column name="loan_term" prompt="TRE_LOAN_CON_WITHDRAWAL_PLAN.LOAN_TERM" renderer="Aurora.formatDate"/>
                                                </a:columns>
                                            </a:grid>
                                        </a:fieldSet>
                                    </a:box>
                                </a:box>
                            </div>
                        </a:fieldSet>
                        <a:fieldSet title="TRE502.FIELDSET.PRINCIPAL_REPAYMENT">
                            <a:box column="1" labelWidth="120" width="280">
                                <a:comboBox name="repayment_method_desc" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.REPAYMENT_METHOD" readOnly="true"/>
                            </a:box>
                            <div id="WC_lastRepayment" style="display:none;">
                                <a:box column="1" labelWidth="120" width="280">
                                    <a:datePicker name="last_repayment_date" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LAST_REPAYMENT_DATE" readOnly="true"/>
                                </a:box>
                            </div>
                            <div id="WC_planRepayment" style="display:none;">
                                <a:box>
                                    <a:box column="4" labelWidth="120" width="896">
                                        <a:datePicker name="first_repayment_date" bindTarget="cal_repayment_plan_ds" prompt="TRE502.FIRST_REPAYMENT_DAY" readOnly="true"/>
                                        <a:comboBox name="repayment_frequency_desc" bindTarget="cal_repayment_plan_ds" prompt="TRE502.REPAYMENT_FREQUENCY" readOnly="true"/>
                                        <a:numberField name="repayment_plan_times" allowDecimals="false" allowNegative="false" bindTarget="cal_repayment_plan_ds" max="999" prompt="HLS.TIMES" readOnly="true"/>
                                    </a:box>
                                    <a:box style="margin-left:115px;">
                                        <a:fieldSet title="TRE502.FIELDSET.REPAYMENT_SCHEDULE">
                                            <a:grid bindTarget="loan_con_repayment_plan_ds" height="200" navBar="true" width="420">
                                                <a:columns>
                                                    <a:column name="times" prompt="HLS.TIMES" width="60"/>
                                                    <a:column name="repayment_type_desc" prompt="TRE503.REPAYMENT_TYPE" renderer="type_renderer"/>
                                                    <a:column name="repayment_date" prompt="TRE_LOAN_CON_REPAYMENT_PLAN.REPAYMENT_DATE" renderer="Aurora.formatDate"/>
                                                    <a:column name="amount" align="right" prompt="TRE_LOAN_CON_REPAYMENT_PLAN.AMOUNT" renderer="Aurora.formatMoney"/>
                                                </a:columns>
                                            </a:grid>
                                        </a:fieldSet>
                                    </a:box>
                                </a:box>
                            </div>
                        </a:fieldSet>
                    </a:tab>
                    <a:tab prompt="合同审签单信息" width="100">
                        <a:form marginWidth="40" title="审签信息">
                            <a:box column="1" width="1120">
                                <a:hBox>
                                    <a:datePicker name="signing_date" bindTarget="loan_contract_ds" prompt="交易时间" readOnly="true" width="150"/>
                                    <a:textField name="loan_contreat_num" bindTarget="loan_contract_ds" prompt="编码" readOnly="true" width="150"/>
                                </a:hBox>
                                <a:hBox>
                                    <a:textField name="transfer_from" bindTarget="loan_contract_ds" prompt="交易对手方" readOnly="true" width="150"/>
                                    <a:textField name="signing_breed" bindTarget="loan_contract_ds" prompt="交易品种" readOnly="true" width="150"/>
                                </a:hBox>
                                <a:hBox>
                                    <a:numberField name="loan_amount" allowFormat="true" allowNegative="false" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LOAN_AMOUNT_CAPITAL" readOnly="true"/>
                                    <a:percentField name="interest_rate" allowNegative="false" bindTarget="loan_contract_ds" prompt="TRE503.INTEREST_RATE" readOnly="true"/>
                                </a:hBox>
                                <a:hBox>
                                    <a:datePicker name="loan_date_from" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LOAN_DATE_FROM" readOnly="true"/>
                                    <a:datePicker name="loan_date_to" bindTarget="loan_contract_ds" prompt="TRE_LOAN_CONTRACT.LOAN_DATE_TO" readOnly="true"/>
                                </a:hBox>
                                <a:hBox>
                                    <a:textField name="loan_usage_description" bindTarget="loan_contract_ds" prompt="资金用途" readOnly="true" width="383"/>
                                </a:hBox>
                                <a:hBox>
                                    <a:textArea name="note" bindTarget="loan_contract_ds" prompt="补充说明" readOnly="true" width="580"/>
                                </a:hBox>
                            </a:box>
                        </a:form>
                    </a:tab>
                    <a:tab prompt="附件资料" width="100">
                        <a:form marginWidth="40" title="附件信息">
                            <a:screen-include screen="atm_query.screen?document_id=${/parameter/@loan_contract_id}&amp;document_table=TRE_LOAN_CONTRACT&amp;showflag=N"/>
                        </a:form>
                    </a:tab>
                </a:tabs>
            </a:tabPanel>
        </a:screenBody>
    </a:view>
</a:screen>
