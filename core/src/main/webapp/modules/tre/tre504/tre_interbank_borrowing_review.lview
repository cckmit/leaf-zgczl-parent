        <script><![CDATA[
            Aurora.Masker.mask(document.documentElement, '${l:HLS.LOADING}');
            
            function lock_current_window() {
                Aurora.Masker.mask(document.documentElement, '${l:HLS.EXECUTING}');
            }
            
            function unlock_current_window() {
                Aurora.Masker.unmask(document.documentElement);
            }
            
            function tre503_loan_review() {
                lock_current_window();
                var result_ds = $('loan_contract_ds');
                var record = result_ds.getCurrentRecord();
                var loan_contract_id = record.get('loan_contract_id');
                var ajax_flag = 'N';
                var win = Aurora.showConfirm('${l:HLS.PROMPT}', '${l:TRE503.IS_LOAN_COFIRM}' + record.get('loan_contract_number') + '${l:TRE503.IS_THIS_CONTRACT}', function() {
                    ajax_flag = 'Y';
                    Aurora.request({
                        url: $('tre_loan_contract_review_link_id').getUrl(),
                        para: {loan_contract_id:loan_contract_id},
                        success: function(res) {
                            result_ds.query(result_ds.currentPage);
                            Aurora.SideBar.show({
                                msg: '操作成功',
                                duration: 2000
                            });
                            window.location.href = $('tre_loan_con_review_maintain_link_id').getUrl();
                            unlock_current_window();
                        },
                        failure: function() {
                            unlock_current_window();
                        },
                        error: function() {
                            unlock_current_window();
                        },
                        scope: this
                    });
                }, function() {
                    unlock_current_window();
                });
                win.on('close', function() {
                    if (ajax_flag == 'N') {
                        unlock_current_window();
                    }
                });
            }
            
            function wcl_save() {
                lock_current_window();
                var head_ds = $('loan_contract_ds'),
                    head_record = head_ds.getCurrentRecord();
                if (head_ds.validate()) {
                    var calc_record = $('cal_repayment_plan_ds').getCurrentRecord();
                    if (calc_record) {
                        head_record.set('first_repayment_date', calc_record.get('first_repayment_date'));
                        head_record.set('repayment_frequency', calc_record.get('repayment_frequency'));
                        head_record.set('repayment_plan_times', calc_record.get('repayment_plan_times'));
                    }
                    head_ds.submit($('tre_loan_contract_save_link_id').getUrl());
                } else {
                    unlock_current_window();
                }
            }
            
