                                        <a:column name="factoring_fee" align="right" editor="repayment_plan_grid_numberField2_id" prompt="保理价值" renderer="Aurora.formatMoney"/>
                                        <a:column name="factoring_fee_pv" align="right" editor="repayment_plan_grid_numberField2_id" prompt="保理现值" renderer="Aurora.formatMoney"/>
                                        <a:column name="residue_rental" align="right" editor="repayment_plan_grid_numberField2_id" prompt="TRE503.RESIDUE_RENTAL" renderer="Aurora.formatMoney"/>
                                     -->
                                    </a:columns>
                                    <a:editors>
                                        <a:lov id="repayment_plan_grid_lov_id">
                                            <a:events>
                                                <a:event name="focus" handler="on_repayment_plan_focus"/>
                                            </a:events>
                                        </a:lov>
                                        <a:textField id="repayment_plan_grid_textfield_id"/>
                                        <a:comboBox id="repayment_plan_grid_combobox_id"/>
                                        <a:numberField id="repayment_plan_grid_numberField1_id" allowDecimals="false" allowNegative="false"/>
                                        <a:numberField id="repayment_plan_grid_numberField2_id" allowNegative="false"/>
                                        <a:datePicker id="repayment_plan_grid_datepicker_id"/>
                                    </a:editors>
                                </a:grid>
                            </a:box>
                        </a:fieldSet>
                    </a:tab>
