<a:screen xmlns:s="aurora.plugin.script" xmlns:a="http://www.aurora-framework.org/application">
    <a:init-procedure>
        <a:model-query defaultWhereClause="t1.document_category=&apos;LOAN_CONTRACT&apos; and t1.business_type=&apos;FACTORING&apos;" fetchAll="true" model="basic.hls_document_type_for_lov" rootPath="document_type_path"/>
        <a:model-query defaultWhereClause="t.code=&apos;TRE502_REPAYMENT_TYPE&apos; and t.code_value in (&apos;FACTORING&apos;,&apos;FACTORING_FEE&apos;) and t.code_enabled_flag=&apos;Y&apos; and t.code_value_enabled_flag=&apos;Y&apos;" fetchAll="true" model="sys.sys_code_values_v" rootPath="repayment_type_path"/>
    </a:init-procedure>
    <a:view>
        <a:link id="tre_loan_con_print_excel_link_id" url="${/request/@context_path}/modules/tre/tre301/tre_loan_con_factor_excel.svc"/>
        <a:link id="tre_loan_contract_repayment_plan_import_link_id" url="${/request/@context_path}/modules/tre/tre503/tre_loan_contract_repayment_plan_import.screen"/>
        <a:link id="tre_factoring_contract_maintain_link_id" url="${/request/@context_path}/modules/tre/tre503/tre_factoring_contract_maintain.screen"/>
        <a:link id="tre_loan_contract_maintain_link_id" url="${/request/@context_path}/modules/tre/tre503/tre_loan_contract_maintain.screen"/>
        <a:link id="tre_loan_con_rpt_plan_calc_interest_link_id" url="${/request/@context_path}/modules/tre/tre503/tre_loan_con_rpt_plan_calc_interest.screen"/>
        <script><![CDATA[
            Aurora.Masker.mask(document.documentElement, '${l:HLS.LOADING}');
            
            function lock_current_window() {
                Aurora.Masker.mask(document.documentElement, '${l:HLS.EXECUTING}');
            }
            
            function unlock_current_window() {
                Aurora.Masker.unmask(document.documentElement);
            }
            
            function tre_factoring_repayment_plan_import() {
                lock_current_window();
                var record = $('loan_contract_ds').getCurrentRecord();
                var loan_contract_id = record.get('loan_contract_id');
                if (!loan_contract_id) {
                    Aurora.showMessage('${l:PROMPT}', '${l:TRE503.SAVE_FIRST}');
                    unlock_current_window();
                    return;
                }
                var win = new Aurora.Window({
                    id: 'tre_loan_contract_repayment_plan_import_winid',
                    params: {
                        loan_contract_id: loan_contract_id,
                        winid: 'tre_loan_contract_repayment_plan_import_winid',
                        dsid: 'loan_contract_ds',
                        validate_source: 'CREATE'
                    },
                    url: $('tre_loan_contract_repayment_plan_import_link_id').getUrl(),
                    title: '${l:TRE503.REPAYMENT_PLAN_IMPORT}',
                    width: 420,
                    height: 275
                });
                win.on('close', function() {
                    unlock_current_window();
                });
            }
            
            function wcl_save() {
                lock_current_window();
                var head_ds = $('loan_contract_ds'),
                    head_record = head_ds.getCurrentRecord();
                if ($('loan_contract_ds').validate()) {
                    var credit_contract_id=head_record.get('bank_credit_contract_id');
                    // alert(credit_contract_id);
                    if(!Ext.isEmpty(credit_contract_id)&&typeof(credit_contract_id)!='undefined'){
                        var available_amount=head_record.get('available_amount'),
                            loan_amount=head_record.get('loan_amount');
                        // alert(loan_amount+'-'+available_amount);
                        if(loan_amount>available_amount){
                            // alert(loan_amount-available_amount);
                            Aurora.showMessage('提示', '借款金额不能大于授信剩余额度！', null, 350);
                            unlock_current_window();
                            return;
                        }
                    }
                    $('loan_contract_ds').submit('tre_loan_contract_save.svc');
                } else {
                    unlock_current_window();
                }
            }
            
            function wcl_goBack() {
                window.location.href = $('tre_loan_contract_maintain_link_id').getUrl();
            }
            
            function loan_contract_ds_update(dataSet, record, name, value, oldvalue) {
                if (name == 'factoring_financing_method') {
                    record.set('discount_rate', '');
                    record.set('bank_financing_scale', '');
                    record.set('financing_scale', '');
                    record.getField('interest_rate').setRequired(false);
                    record.set('interest_rate', '');
                    record.set('base_rate', '');
                    if (value == 'DISCOUNT') {
                        record.getField('discount_rate').setRequired(true);
                        record.getField('bank_financing_scale').setRequired(false);
                        record.getField('financing_scale').setRequired(false);
                        record.getField('interest_rate').setRequired(true);
                        record.getField('base_rate').setRequired(false);
                        Ext.fly('FC_Discount').setStyle({
                            display: ''
                        });
                        Ext.fly('FC_Prepay').setStyle({
                            display: 'none'
                        });
                    } else if (value == 'PREPAID') {
                        record.getField('discount_rate').setRequired(false);
                        record.getField('bank_financing_scale').setRequired(false);
                        record.getField('financing_scale').setRequired(false);
                        record.getField('interest_rate').setRequired(true);
                        record.getField('base_rate').setRequired(false);
                        Ext.fly('FC_Discount').setStyle({
                            display: 'none'
                        });
                        Ext.fly('FC_Prepay').setStyle({
                            display: ''
                        });
                    }
                } else if (name == 'bank_branch_id') {
                    record.set('bank_account_id', '');
                    record.set('withdraw_account_id', '');
                    record.set('bank_account_code', '');
                    record.set('withdraw_account_code', '');
                    record.set('bank_account_num', '');
                    record.set('withdraw_account_num', '');
                    record.set('currency', '');
                    record.set('currency_desc', '');
                } else if (name == 'bank_account_id') {
                    if (Ext.isEmpty(record.get('withdraw_account_id'))) {
                        record.set('withdraw_account_id', value);
                    }
                } else if (name == 'bank_account_code') {
                    if (Ext.isEmpty(record.get('withdraw_account_code'))) {
                        record.set('withdraw_account_code', value);
                    }
                } else if (name == 'bank_account_num') {
                    if (Ext.isEmpty(record.get('withdraw_account_num'))) {
                        record.set('withdraw_account_num', value);
                    }
                }
            }
            
            function loanConSaveSuccess(dataSet, res) {
                var record = dataSet.getCurrentRecord();
                //window.location.href = $('tre_factoring_contract_maintain_link_id').getUrl() + '?loan_contract_id=' + record.get('loan_contract_id') + '&business_type=' + record.get('business_type');
                unlock_current_window();
                $('factoring_repayment_plan_ds').query();
                $('loan_contract_ds').query();
                $('atm_upload_ds').query();
            }
            
            function loanConSaveSubmitFailed() {
                unlock_current_window();
            }
            
            function loanConDsLoad(ds) {
                var record = ds.getCurrentRecord(),
                    financing_method = record.get('factoring_financing_method');
                if (financing_method == 'DISCOUNT') {
                    record.getField('discount_rate').setRequired(true);
                    record.getField('bank_financing_scale').setRequired(false);
                    record.getField('financing_scale').setRequired(false);
                    record.getField('interest_rate').setRequired(true);
                    record.getField('base_rate').setRequired(false);
                    Ext.fly('FC_Discount').setStyle({
                        display: ''
                    });
                    Ext.fly('FC_Prepay').setStyle({
                        display: 'none'
                    });
                } else {
                    record.getField('discount_rate').setRequired(false);
                    record.getField('bank_financing_scale').setRequired(false);
                    record.getField('financing_scale').setRequired(false);
                    record.getField('interest_rate').setRequired(true);
                    record.getField('base_rate').setRequired(false);
                    Ext.fly('FC_Discount').setStyle({
                        display: 'none'
                    });
                    Ext.fly('FC_Prepay').setStyle({
                        display: ''
                    });
                }
                unlock_current_window();
            }
            
            function set_factoring_repayment_plan_field(value, record, fromLoad) {
                if (!fromLoad) {
                    record.set('times', '');
                    record.set('rental', '');
                    record.set('principal', '');
                    record.set('interest', '');
                }
                if (value == 'FACTORING') {
                    record.getField('times').setRequired(true);
                    record.getField('rental').setRequired(false);
                    record.getField('principal').setRequired(true);
                    record.getField('interest').setRequired(true);
                } else {
                    record.getField('times').setRequired(false);
                    record.getField('rental').setRequired(false);
                    record.getField('principal').setRequired(false);
                    record.getField('interest').setRequired(false);
                }
            }
            
            function on_factoring_repayment_plan_update(ds, record, name, value, old_value) {
                if (name == 'repayment_type') {
                    set_factoring_repayment_plan_field(value, record);
                } else if (name == 'principal' || name == 'interest') {
                    var principal = record.get('principal') || 0,
                        interest = record.get('interest') || 0;
                    record.set('amount', plus(principal, interest));
                }
            }
            
            function on_factoring_repayment_plan_load(ds) {
                var records = ds.getAll();
                for (var i = 0;i < records.length;i++) {
                    var record = records[i];
                    set_factoring_repayment_plan_field(record.get('repayment_type'), record, true);
                }
            }
            
            function on_factoring_repayment_plan_add(ds, record, index) {
                var head_record = $('loan_contract_ds').getCurrentRecord();
                record.set('loan_contract_id', head_record.get('loan_contract_id'));
            }
            
            function on_repayment_plan_grid_editorfunction(record, name) {
                var repayment_type = record.get('repayment_type');
                if (repayment_type == 'FACTORING') {
                    return 'repayment_plan_grid_lov_id';
                } else if (repayment_type == 'FACTORING_FEE') {
                    return 'repayment_plan_grid_textfield_id';
                } else {
                    return '';
                }
            }
            
            function on_repayment_plan_focus(object) {
                var record = object.record || object.binder.ds.create();
                var display_name = object.binder.name,
                    field = record.getField(display_name);
                if (display_name == 'times') {
                    var loan_currentrecord = $('loan_contract_ds').getCurrentRecord();
                    if (loan_currentrecord) {
                        field.setLovPara('contract_id', loan_currentrecord.get('contract_id'));
                    }
                }
            }
            
            function dateValidator(record, name, value) { //日期校验方法
                if (name == 'loan_date_from' || name == 'loan_date_to') {
                    var start_date = Aurora.formatDate(record.get('loan_date_from'));
                    var end_date = Aurora.formatDate(record.get('loan_date_to'));
                    if ( !! end_date) { //由于结束日期非必填，只有在结束日期填了才进行比较
                        if (!compareDate(start_date, end_date)) {
                            return '${l:TRE503.LOAN_DATE_FROM_BEYOND_TO}'; //校验不通过返回字符串
                        }
                    }
                    return true; //校验通过返回true
                }
            }
            
            function on_bank_account_focus(object) {
                var display_name = object.binder.name,
                    record = object.record || object.binder.ds.create();
                record.getField(display_name).setLovPara('bank_branch_id', record.get('bank_branch_id') || -999);
            }
            
            function compareDate(start, end) {
                if (start > end) {
                    return false;
                }
                return true;
            }
            function tre_factoring_rpt_plan_calc_interest() {
                lock_current_window();
                var record = $('loan_contract_ds').getCurrentRecord();
                var loan_contract_id = record.get('loan_contract_id');
                if (!loan_contract_id) {
                    Aurora.showMessage('${l:PROMPT}', '${l:TRE503.SAVE_FIRST}');
                    unlock_current_window();
                    return;
                }
                var win = new Aurora.Window({
                    id: 'tre_loan_con_rpt_plan_calc_interest_winid',
                    params: {
                        loan_contract_id: loan_contract_id,
                        winId: 'tre_loan_con_rpt_plan_calc_interest_winid'
                    },
                    url: $('tre_loan_con_rpt_plan_calc_interest_link_id').getUrl(),
                    title: '${l:TRE503.CALC_INTEREST}',
                    width: 420,
                    height: 255
                });
                win.on('close', function() {
                    unlock_current_window();
                });
            
            }
            function type_renderer(value, record, name) {
                if(name=='repayment_type_desc'){
                    if(record.get('repayment_type')=='LOAN_CHARGE'||record.get('repayment_type')=='OTHER_FEE'||record.get('repayment_type')=='CONSULTING_FEE'){
                        record.getField('expire_date').setRequired(false);
                        record.set('times',0);
                    }
                }
                return value;
            }
            
            function tre_wfl_submit() {
                lock_current_window();
                if ($('loan_contract_ds').validate()) {
                    var record = $('loan_contract_ds').getCurrentRecord();
                    var loan_contract_id = record.get('loan_contract_id');
                    Aurora.showConfirm('${HLS.PROMPT}', '${l:HLS.ARE_YOU_SURE_TO_SUBMIT}', function() {
                        Aurora.request({
                            url: '${/request/@context_path}/autocrud/tre.tre503.tre_con_wfl_submit/update',
                            para: {
                                loan_contract_id: loan_contract_id
                            },
                            success: function(res) {
                                unlock_current_window();
                                Aurora.SideBar.show({
                                    msg: '${l:HLS.SUBMIT_SUCCESS}',
                                    duration: 2000
                                });
                                wcl_goBack();
                            },
                            failure: function() {
                                unlock_current_window();
                            },
                            error: function() {
                                unlock_current_window();
                            },
                            scope: this
                        });
                    }, function() {
                        unlock_current_window();
                    });
                }else{
                     unlock_current_window();
                }
            }
            
            function tre_loan_con_print_excel_fun() {
                var url;
                var ds = $('loan_contract_ds');
                var record = ds.getCurrentRecord();
                url = $('tre_loan_con_print_excel_link_id').getUrl() + '?loan_contract_id=' +record.get('loan_contract_id');
                window.open(url);
            }
        ]]></script>
        <a:dataSets>
            <a:dataSet id="document_type_name_ds">
                <a:datas dataSource="/model/document_type_path"/>
            </a:dataSet>
            <a:dataSet id="repayment_type_ds">
                <a:datas dataSource="/model/repayment_type_path"/>
            </a:dataSet>
            <a:dataSet id="loan_contract_type_ds" lookupCode="TRE502_LOAN_CONTRACT_TYPE"/>
            <a:dataSet id="yes_no_ds" lookupCode="YES_NO"/>
            <a:dataSet id="factoring_loan_amount_type_ds" lookupCode="TRE502_FACTORING_LOAN_AMOUNT_TYPE"/>
            <a:dataSet id="factoring_financing_method_ds" lookupCode="TRE502_FACTORING_FINANCING_METHOD"/>
            <a:dataSet id="payment_method_ds" lookupCode="TRE502_PAYMENT_METHOD"/>
            <a:dataSet id="loan_contract_ds" autoQuery="true" model="tre.tre502.tre_loan_contract" queryUrl="${/request/@context_path}/autocrud/tre.tre502.tre_loan_contract/query?loan_contract_id=${/parameter/@loan_contract_id}">
                <a:fields>
                    <a:field name="document_type_desc" displayField="description" options="document_type_name_ds" required="true" returnField="document_type" valueField="document_type"/>
                    <!-- <a:field name="loan_contract_type" defaultValue="${/parameter/@loan_contract_type}"/>
                    <a:field name="loan_contract_type_desc" displayField="code_value_name" options="loan_contract_type_ds" readOnly="true" returnField="loan_contract_type" valueField="code_value"/> -->
                    <a:field name="loan_contract_number" required="true"/>
                    <a:field name="bank_credit_contract_id"/>
                    <a:field name="bank_credit_contract_number" lovAutoQuery="true" lovGridHeight="270" lovHeight="400" lovService="tre.tre501.tre_bank_credit_contract_lov" lovWidth="700" title="TRE_LOAN_CONTRACT.BANK_CREDIT_CONTRACT_ID">
                        <a:mapping>
                            <a:map from="credit_contract_number" to="bank_credit_contract_number"/>
                            <a:map from="credit_contract_id" to="bank_credit_contract_id"/>
                            <a:map from="available_amount" to="available_amount"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="bank_branch_code" lovGridHeight="350" lovHeight="510" lovLabelWidth="90" lovService="basic.csh_bank_branch_for_lov" lovWidth="580" prompt="HLS.BANK_CODE" required="true" title="HLS.BANK_CODE">
                        <a:mapping>
                            <a:map from="bank_branch_id" to="bank_branch_id"/>
                            <a:map from="bank_branch_code" to="bank_branch_code"/>
                            <a:map from="bank_branch_name" to="bank_branch_name"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="bank_branch_id" required="true"/>
                    <a:field name="bank_branch_name" prompt="HLS.BANK_NAME" readOnly="true"/>
                    <a:field name="bank_account_code" lovGridHeight="350" lovHeight="510" lovLabelWidth="90" lovService="csh.CSH101.csh_bank_account_lov" lovWidth="580" required="true" title="CSH511.BANK_ACCOUNT_CODE">
                        <a:mapping>
                            <a:map from="bank_account_id" to="bank_account_id"/>
                            <a:map from="bank_account_code" to="bank_account_code"/>
                            <a:map from="bank_account_num" to="bank_account_num"/>
                            <a:map from="currency_code" to="currency"/>
                            <a:map from="currency_name" to="currency_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="bank_account_id" required="true"/>
                    <a:field name="bank_account_num" readOnly="true"/>
                    <a:field name="withdraw_account_code" lovGridHeight="350" lovHeight="510" lovLabelWidth="90" lovService="csh.CSH101.csh_bank_account_lov" lovWidth="580" required="true" title="CSH511.BANK_ACCOUNT_CODE">
                        <a:mapping>
                            <a:map from="bank_account_id" to="withdraw_account_id"/>
                            <a:map from="bank_account_code" to="withdraw_account_code"/>
                            <a:map from="bank_account_num" to="withdraw_account_num"/>
                            <a:map from="currency_code" to="currency"/>
                            <a:map from="currency_name" to="currency_desc"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="note"/>
                    <a:field name="withdraw_account_id" required="true"/>
                    <a:field name="withdraw_account_num" readOnly="true"/>
                    <a:field name="loan_certificate_flag_desc" displayField="code_value_name" options="yes_no_ds" required="true" returnField="loan_certificate_flag" valueField="code_value"/>
                    <a:field name="related_party_flag_desc" displayField="code_value_name" options="yes_no_ds" required="true" returnField="related_party_flag" valueField="code_value"/>
                    <a:field name="currency"/>
                    <a:field name="currency_desc" readOnly="true"/>
                    <a:field name="annual_days"/>
                    <a:field name="loan_amount"/>
                    <a:field name="loan_amount_type_desc" displayField="code_value_name" options="factoring_loan_amount_type_ds" required="true" returnField="loan_amount_type" valueField="code_value"/>
                    <a:field name="factoring_financing_method_desc" displayField="code_value_name" options="factoring_financing_method_ds" returnField="factoring_financing_method" valueField="code_value"/>
                    <a:field name="discount_rate"/>
                    <a:field name="financing_scale"/>
                    <a:field name="interest_rate"/>
                    <a:field name="payment_method_desc" displayField="code_value_name" options="payment_method_ds" returnField="payment_method" valueField="code_value"/>
                    <a:field name="payment_date" required="true"/>
                    <a:field name="bank_financing_scale" required="false"/>
                    <a:field name="base_rate" required="false"/>
                    <a:field name="total_repayment" readOnly="true"/>
                    <a:field name="total_interest" readOnly="true"/>
                    <a:field name="total_fee_pv" readOnly="true"/>
                    <a:field name="total_fee_pv" readOnly="true"/>
                    <a:field name="net_finance_amount" readOnly="true"/>
                    <a:field name="interest_margin" readOnly="true"/>
                    <a:field name="loan_date_from" required="true" validator="dateValidator"/>
                    <a:field name="loan_date_to" required="true" validator="dateValidator"/>
                    <a:field name="contract_number" lovGridHeight="350" lovHeight="500" lovService="cont.CON500.con_contract_lov" lovWidth="500" required="true" title="HLS.CONTRACT">
                        <a:mapping>
                            <a:map from="contract_id" to="contract_id"/>
                            <a:map from="vat_rate_of_interest" to="vat_rate_of_interest"/>
                            <a:map from="contract_number" to="contract_number"/>
                            <a:map from="contract_name" to="contract_name"/>
                        </a:mapping>
                    </a:field>
                    <a:field name="contract_id"/>
                    <a:field name="vat_rate_of_interest"/>
                    <a:field name="contract_name" readOnly="true"/>
                    <a:field name="consulting_fee" required="true"/>
                    <a:field name="loan_charge" required="true"/>
                    <a:field name="other_fee"/>
                    <a:field name="deposit_amount" required="true"/>
                    <a:field name="bank_branch_bp_id_n" lovHeight="500" lovService="tre.bank_branch_bp_lov" lovWidth="500" required="true" title="商业伙伴">
