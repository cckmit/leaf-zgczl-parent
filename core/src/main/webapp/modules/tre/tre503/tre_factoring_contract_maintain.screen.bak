                                        <a:column name="amount" align="right" editor="repayment_plan_grid_numberField2_id" prompt="TRE503.AMOUNT" renderer="Aurora.formatMoney"/>
                                        <a:column name="principal" align="right" editor="repayment_plan_grid_numberField2_id" prompt="TRE503.PRINCIPAL" renderer="Aurora.formatMoney"/>
                                        <a:column name="interest" align="right" editor="repayment_plan_grid_numberField2_id" prompt="TRE502.REPAYMENT_INTEREST" renderer="Aurora.formatMoney"/>
