            window['${/parameter/@layout_code}_submit_approval_layout_dynamic_click'] = function() {
                Leaf.SideBar.enable = false;
                window['${/parameter/@layout_code}_SAVE_LAYOUT_DYNAMIC_CLICK'](csh_payment_req_submit_finally);
