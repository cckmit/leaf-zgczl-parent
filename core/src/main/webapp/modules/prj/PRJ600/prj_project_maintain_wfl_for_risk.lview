        <script><![CDATA[
            function go_to_opinion_page() {
                // debugger;
                if ('${/model/law_audit_opinion/record/@status}' != 'SUBMIT') {
                    Leaf.showMessage('提示', '法务未提交！');
                    return;
                }
            
                url = '';
                if ('${/parameter/@show_opinion}' == 'READONLY') {
                    url = $('prj_opinion_readonly_link').getUrl() + '?project_id=${/parameter/@project_id}';
                } else if ('${/parameter/@show_opinion}' == 'WRITABLE') {
                    url = $('prj_opinion_writable_link').getUrl() + '?project_id=${/parameter/@project_id}';
                } else if ('${/parameter/@show_opinion}' == 'WRITABLE_BUSINESS') {
                    url = $('prj_opinion_writable_link').getUrl() + '?project_id=${/parameter/@project_id}&node_info=BUSINESS';
                }
            
                new Leaf.Window({
                    id: 'prj_opinion_window',
                    title: '法务部项目法审意见单',
                    url: url,
                    fullScreen: true
                });
            }
            
            function go_to_chance_info() {
                var maintain_type = 'READONLY';
                var param = {};
                param['function_code'] = 'PRJ301';
                param['chance_id'] = ${/model/prj_basic_info/record/@chance_id};
                param['document_id'] = ${/model/prj_basic_info/record/@chance_id};
                param['document_category'] = '${/model/prj_basic_info/record/@chance_document_category}';
                param['document_type'] = '${/model/prj_basic_info/record/@chance_document_type}';
                param['function_usage'] = 'QUERY';
                param['maintain_type'] = maintain_type;
                param['url_title'] = '${l:PRJ301.PRJ_CHANCE_QUERY}';
                hls_doc_get_layout_code('con_contract_get_layout_code_link_id', param, 'pageLink_nanshan_all_layout');
            }
            
            
            function go_to_project_info() {
                var maintain_type = 'READONLY';
                var record = $('prj600_project_basic_info_ds').getAt(0);
                var param = record.data;
                param['document_id'] = '${/parameter/@project_id}';
                param['layout_code'] = '${/parameter/@layout_code}';
                param['calc_type'] = '${/parameter/@calc_type}';
                param['function_code'] = '${/parameter/@function_code}';
                param['function_usage'] = 'QUERY';
                param['maintain_type'] = maintain_type;
                param['url_title'] = '${l:HLS.PROJECT_MAITAIN}';
                param['prj_report_read_flag'] = 'Y';
                param['prj_bp_id'] = record.get('prj_bp_id');
                hls_doc_get_layout_code('prj_project_get_layout_code_link_id', param, 'prj_project_modify_link');
            }
            
            function go_to_risk_evaluate_page() {
                new Leaf.Window({
                    id: 'prj_risk_window',
                    url: '${/request/@context_path}/modules/fnd/FND714/fnd_sc_score_query.screen?project_id=${/parameter/@project_id}',
                    title: '信用风险量化评估',
                    fullScreen: true
                });
            }
            
            function go_to_risk_review_page() {
                url = '';
               var lease_channel = '${/model/prj_basic_info/record/@lease_channel}';
               var history_or_new = '${/model/prj_basic_info/record/@history_or_new}';
                 if ('${/parameter/@show_risk}' == 'READONLY') {
                  if (lease_channel == '01' && history_or_new == 'Y') {
                      url = $('prj_risk_review_readonly_small_link').getUrl();
                  } else {
                      url = $('prj_risk_review_readonly_link').getUrl(); 
                  }
                } else if ('${/parameter/@show_risk}' == 'WRITABLE') {
                    
                  if (lease_channel == '01' && history_or_new == 'Y') {
                      url = $('prj_risk_review_writable_small_link').getUrl();
                  } else {
                      url = $('prj_risk_review_writable_link').getUrl();   
                  } 
                }
            
                 // 初始化数据 2018-04-17 by 9796
                Leaf.request({
                    url: '${/request/@context_path}/autocrud/prj.PRJ501N.prj_wfl_risk_review_update/update',
                    para: {
                        project_id: '${/parameter/@project_id}'
                    },
                    success: function(res) {
            
                        new Leaf.Window({
                            id: 'prj_risk_review_window',
                            url: url + '?project_id=${/parameter/@project_id}',
                            title: '风险审查报告',
                            fullScreen: true
                        });
            
                    },
                    scope: this
                }); 
            }
            
            function winOpen_projectDetailInfo() {
                var record = $('prj600_project_basic_info_ds').getAt(0);
                var param = record.data;
                param['function_code'] = 'PRJ600Q';
                param['document_id'] = '${/parameter/@project_id}';
                param['function_usage'] = 'QUERY';
                param['maintain_type'] = 'QUERY';
                param['url_title'] = '项目详细信息';
                hls_doc_get_layout_code('prj_project_get_layout_code_link_id', param, 'prj_project_query_link', 'prj600_project_basic_info_ds');
            }
            
            function prj600_project_assign_update(ds, record, name, value, old_value) {
                if (name == 'employee_id') {
                    var instance_id = '${/parameter/@instance_id}';
                    var approval_type = '${/parameter/@approval_type}';
                    var project_id = '${/parameter/@project_id}';
            
                    Leaf.request({
                        url: $('prj600_emp_assign_id').getUrl(),
                        para: {
                            instance_id: instance_id,
                            approval_type: approval_type,
                            employee_id: value,
                            project_id: project_id
                        },
                        success: function() {
            
                           },
                        failure: function() {
            
                           },
                        error: function() {
            
                           },
                        scope: this
                    });
                }
            }
            
            function prj600_on_business_comment_add(ds, record, index) {
                if ('${/parameter/@show_risk_grid}' == 'READONLY' || '${/parameter/@show_risk_grid}' == 'WRITABLE') {
                    record.set('comment_node_type', 'RISK_ANSWER');
                }
            
                if ('${/parameter/@show_law_grid}' == 'READONLY' || '${/parameter/@show_law_grid}' == 'WRITABLE') {
                    record.set('comment_node_type', 'LEGAL_ANSWER');
                }
            }
            
            function prj600_on_business_comment_load(ds) {
                var records = ds.getAll();
            
                for (var i = 0;i < records.length;i++) {
                    records[i].getField('column_desc').setReadOnly(true);
                    records[i].getField('comment_date').setReadOnly(true);
                    records[i].getField('comment_data').setReadOnly(true);
                }
            }
            
            function prj600_on_risk_comment_load(ds) {
                var records = ds.getAll();
            
                for (var i = 0;i < records.length;i++) {
                    records[i].getField('column_desc').setReadOnly(true);
                    records[i].getField('comment_date').setReadOnly(true);
                    records[i].getField('comment_data').setReadOnly(true);
                }
            }
            
            function prj600_on_law_comment_load(ds) {
                var records = ds.getAll();
            
                for (var i = 0;i < records.length;i++) {
                    records[i].getField('column_desc').setReadOnly(true);
                    records[i].getField('comment_date').setReadOnly(true);
                    records[i].getField('comment_data').setReadOnly(true);
                }
            }
            
            function prj600_file_upload_renderer(value, record, name) {
                return '<a href=javascript:prj600_file(' + record.get('comment_id') + ',true)>附件上传</a>';
            }
            
            function prj600_file_check_renderer(value, record, name) {
                return '<a href=javascript:prj600_file(' + record.get('comment_id') + ',false)>附件查看</a>';
            }
            
            function prj600_file(comment_id, isUpload) {
                var url = '';
                var title = '';
                if (isUpload == 'true') {
                    url = $('prj501_file_upload').getUrl() + '?table_name=PRJ_APPROVAL_NODE_COMMENT&header_id=' + comment_id;
                    title = '附件上传';
                } else {
                    url = $('prj501_file_check').getUrl() + '?table_name=PRJ_APPROVAL_NODE_COMMENT&header_id=' + comment_id;
                    title = '附件查看';
                }
                new Leaf.Window({
                    url: url,
                    title: title,
                    id: 'prj501_law_uploadFile_id',
                    width: 850,
                    height: 400
                });
            }
            
            function prj600_detail_lease_item_link(lease_item_type, project_lease_item_id) {
                new Leaf.Window({
                    id: 'receipt_print_winid',
                    url: '${/request/@context_path}/modules/prj/PRJ501N/prj_lease_item_detail.screen',
                    params: {
                        project_lease_item_id: project_lease_item_id,
                        lease_type: lease_item_type + '_QUERY'
                    },
                    title: '租赁物明细',
                    fullScreen: true
                });
            }
            
            function prj600_detail_credit_link(credit_id) {
                new Leaf.Window({
                    id: 'hls219_bp_grant_credit_line_window',
                    url: $('hls219_bp_grant_credit_line_link').getUrl(),
                    width: 730,
                    height: 400,
                    params: {
                        credit_id: credit_id,
                        readonly_flag: 'Y'
                    },
                    title: '授信明细'
                });
            }
            
            function file_render(value, record, name) {
                if (value != null) {
                    var link = '${/request/@context_path}/atm_download.svc?attachment_id=';
                    var str = value.split(';;');
                    var url = '';
                    var file_name;
                    for (var i = 0;i < str.length;i++) {
                        var temp = str[i].split('--');
                        if (!Leaf.isEmpty(temp[0])) {
                            file_name = temp[0].toUpperCase();
                            if (file_name.indexOf('.PDF') >= 0) {
                                url = '<a href=javascript:view_pdf(\'' + temp[1] + '\')>' + temp[0] + '</a>';
                            } else {
                                url = url + '<a href=' + link + temp[1] + ' ref="img">' + temp[0] + '</a>' + ',';
                            }
                        }
                    }
                    return url;
                }
            }
            
            function view_pdf(attachment_id) {
                debugger;
                Leaf.request({
                    url: '${/request/@context_path}/autocrud/fnd.fnd_atm_attachment/query',
                    para: {
                        attachment_id: attachment_id
                    },
                    success: function(res) {
                        var path = res.result.record.file_path;
                        path = path.substr(path.indexOf('hls_attachment'));
                        var tomcat_source = '${/parameter/@tomcat_source}';
                        var source_path = 'http://' + window.location.host + '/' + tomcat_source + '/' + path;
                        var oWin = window.open(source_path);
            
                    },
                    scope: this
                });
            }
        ]]></script>
