        <script><![CDATA[
              Leaf.onReady(init);
              
              function init() {       
                    if('${/parameter/@pdf_flag}'!='Y'){
                       document.getElementById("contract_detail_ds_id").style.display = "none";
                   }
                   
                  if (!'${/parameter/@approval_type}') {              
                      
                      document.getElementById("assign_employee_form_id").style.display = "none";
                  } 
                  
                  if ('${/parameter/@show_risk}' != 'READONLY' && '${/parameter/@show_risk}' != 'WRITABLE') {              
                      
                      document.getElementById("prj600_risk_review_id").style.display = "none";
                  } 
                  
                  if ('${/parameter/@show_opinion}' != 'READONLY'&&'${/parameter/@show_opinion}' != 'WRITABLE'&&'${/parameter/@show_opinion}' != 'WRITABLE_BUSINESS') {              
                      
                      document.getElementById("opinion_page_id").style.display = "none";
                  } 
                  
                  if (!'${/parameter/@show_evaluate}') {              
                      
                      document.getElementById("risk_evaluate_page_id").style.display = "none";
                  } 
                  if('${/parameter/@credit_flag}'!='Y'){
                      document.getElementById("prj_wfl_credit_tab_id").style.display = "none";
                  }
                  setBusinessDSPara();
              }
              
              //设置业务反馈的查询参数，如果是在法审工作流中则显示法审回复，反正在风险工作流则显示风险回复
              function setBusinessDSPara(){
                  // debugger;
                  $('prj600_business_comment_ds').setQueryParameter('instance_id','${/parameter/@instance_id}');
                  
                  if('${/parameter/@show_risk_grid}'=='READONLY'||'${/parameter/@show_risk_grid}'=='WRITABLE'){
                      $('prj600_business_comment_ds').setQueryParameter('comment_node_type','RISK_ANSWER');
                  }
                  else if('${/parameter/@show_law_grid}'=='READONLY'||'${/parameter/@show_law_grid}'=='WRITABLE'){
                      $('prj600_business_comment_ds').setQueryParameter('comment_node_type','LEGAL_ANSWER');
                  }
                  
                  $('prj600_business_comment_ds').query();
              }
          ]]></script>
