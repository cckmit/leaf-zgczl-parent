        <a:dataSets>
            <a:dataSet id="prj600_project_basic_info_ds" autoQuery="true" model="prj.PRJ600.prj_project_info" queryUrl="${/request/@context_path}/autocrud/prj.PRJ600.prj_project_info/query?project_id=${/parameter/@project_id}">
                <a:fields>
                    <a:field name="prj_project_detail_info"/>
                </a:fields>
            </a:dataSet>
            <a:dataSet id="prj600_project_assign_employee" autoQuery="true" model="prj.PRJ600.prj_meeting_assign_wfl" queryUrl="${/request/@context_path}/autocrud/prj.PRJ600.prj_meeting_assign_wfl/query?instance_id=${/parameter/@instance_id}&amp;approval_type=${/parameter/@approval_type}">
                <a:fields>
                    <a:field name="employee_name" lovGridHeight="200" lovHeight="400" lovService="prj.PRJ600.exp_emp_assign_e_v?approval_type=${/parameter/@approval_type}" lovWidth="450" required="true" title="EXP_ORG_UNIT.PARENT_UNIT">
                        <a:mapping>
                            <a:map from="employee_id" to="employee_id"/>
                            <a:map from="employee_name" to="employee_name"/>
                        </a:mapping>
                    </a:field>
                </a:fields>
                <a:events>
                    <a:event name="update" handler="prj600_project_assign_update"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="prj600_project_sign_condition" autoQuery="true" model="prj.PRJ600.prj_sign_condition" queryUrl="${/request/@context_path}/autocrud/prj.PRJ600.prj_sign_condition/query?project_id=${/parameter/@project_id}"/>
            <a:dataSet id="prj600_project_pay_condition" autoQuery="true" model="prj.PRJ600.prj_pay_condition" queryUrl="${/request/@context_path}/autocrud/prj.PRJ600.prj_pay_condition/query?project_id=${/parameter/@project_id}"/>
            <a:dataSet id="prj600_project_paid_affairs" autoQuery="true" model="prj.PRJ600.prj_paid_affairs" queryUrl="${/request/@context_path}/autocrud/prj.PRJ600.prj_paid_affairs/query?project_id=${/parameter/@project_id}"/>
            <a:dataSet id="prj600_project_other_affairs" autoQuery="true" model="prj.PRJ600.prj_other_affairs" queryUrl="${/request/@context_path}/autocrud/prj.PRJ600.prj_other_affairs/query?project_id=${/parameter/@project_id}"/>
            <a:dataSet id="prj600_risk_comment_ds" autoQuery="true" model="prj.PRJ600.prj_approval_node_comment" queryUrl="${/request/@context_path}/autocrud/prj.PRJ600.prj_approval_node_comment/query?instance_id=${/parameter/@instance_id}&amp;comment_node_type=RISK" selectable="true">
                <a:fields>
                    <a:field name="column_desc" prompt="序号"/>
                    <a:field name="comment_date" datatype="date" prompt="日期" readOnly="${/parameter/@risk_readonly}"/>
                    <a:field name="comment_data" prompt="问题" readOnly="${/parameter/@risk_readonly}"/>
                    <a:field name="comment_node_type" defaultValue="RISK"/>
                    <a:field name="instance_id" defaultValue="${/parameter/@instance_id}"/>
                </a:fields>
                <a:events>
                    <a:event name="load" handler="prj600_on_risk_comment_load"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="prj600_law_comment_ds" autoQuery="true" model="prj.PRJ600.prj_approval_node_comment" queryUrl="${/request/@context_path}/autocrud/prj.PRJ600.prj_approval_node_comment/query?instance_id=${/parameter/@instance_id}&amp;comment_node_type=LEGAL" selectable="true">
                <a:fields>
                    <a:field name="column_desc" prompt="序号"/>
                    <a:field name="comment_date" datatype="date" prompt="日期" readOnly="${/parameter/@law_readonly}"/>
                    <a:field name="comment_data" prompt="问题" readOnly="${/parameter/@law_readonly}"/>
                    <a:field name="comment_node_type" defaultValue="LEGAL"/>
                    <a:field name="instance_id" defaultValue="${/parameter/@instance_id}"/>
                </a:fields>
                <a:events>
                    <a:event name="load" handler="prj600_on_law_comment_load"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="prj600_business_comment_ds" model="prj.PRJ600.prj_approval_node_comment" selectable="true">
                <a:fields>
                    <a:field name="column_desc" prompt="序号"/>
                    <a:field name="comment_date" datatype="date" prompt="日期" readOnly="${/parameter/@law_readonly}"/>
                    <a:field name="comment_data" prompt="回复" readOnly="${/parameter/@law_readonly}"/>
                    <a:field name="comment_node_type"/>
                    <a:field name="instance_id" defaultValue="${/parameter/@instance_id}"/>
                </a:fields>
                <a:events>
                    <a:event name="add" handler="prj600_on_business_comment_add"/>
                    <a:event name="load" handler="prj600_on_business_comment_load"/>
