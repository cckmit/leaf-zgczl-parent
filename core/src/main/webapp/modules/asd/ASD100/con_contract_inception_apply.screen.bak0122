            }
            
            function renderDate(value, record, name) {
                var tag = record.get('tag');
                if (tag == '-1') {
                    return '<font color="red">' + Aurora.formatDate(value) + '</font>';
                }
                return Aurora.formatDate(value);
            }
            
            function sum_amount(datas,name){
              var sum = 0;
             for(var i=0;i<datas.length;i++){
                 var r = datas[i];
                 var d = r.get(name);
                 var n = parseFloat(d);
                 if(!isNaN(n)){                        
                  sum +=n;
                  }
              }
			return '<font color="red">' + Aurora.formatMoney(sum) + '</font>';     
            }
			
