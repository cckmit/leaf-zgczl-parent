        <a:dataSets>
            <a:dataSet id="report_years_ds" lookupCode="REPORT_YEARS"/>
            <a:dataSet id="report_query_ds" autoCreate="true">
                <a:fields>
                    <a:field name="lease_organization"/>
                    <a:field name="report_month"/>
                    <a:field name="report_year" required="true"/>
                    <a:field name="report_year_desc" displayField="code_value_name" options="report_years_ds" returnField="report_year" valueField="code_value"/>
                </a:fields>
                <a:events>
                    <a:event name="update" handler="update_year"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="report_query_partial_ds" autoCreate="true">
                <a:fields>
                    <a:field name="lease_organization"/>
                    <a:field name="report_month"/>
                    <a:field name="report_year" required="true"/>
                    <a:field name="report_year_desc" displayField="code_value_name" options="report_years_ds" returnField="report_year" valueField="code_value"/>
                </a:fields>
                <a:events>
                    <a:event name="update" handler="update_year"/>
                </a:events>
            </a:dataSet>
            <a:dataSet id="rental_repayment_ds" autoQuery="true" model="rpt.RPT0002.rpt_rental_repayment_query" queryDataSet="report_query_ds">
                <a:fields>
                    <a:field name="total_amount" required="true"/>
                </a:fields>
            </a:dataSet>
            <a:dataSet id="rental_repayment_detail_ds" autoPageSize="true" autoQuery="true" model="rpt.RPT0002.rpt_rental_repayment_query_detail" queryDataSet="report_query_ds"/>
            <a:dataSet id="rental_repayment_detail_partial_ds" autoPageSize="true" autoQuery="true" model="rpt.RPT0002.rpt_rental_repayment_query_detail" queryDataSet="report_query_partial_ds"/>
        </a:dataSets>
