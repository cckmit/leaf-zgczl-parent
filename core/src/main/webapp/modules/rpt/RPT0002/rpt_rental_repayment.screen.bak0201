                    } else if (name == "total_month_amount") {
                        var t_1 = record.get("total_month_amount");
                        var t_2 = parseFloat(t_1);
                        if (!isNaN(t_2)) {
                            sum_total_month += t_2;
                        }
                    } else if (name == "total_amount") {
                        var t_1 = record.get("total_amount");
                        var t_2 = parseFloat(t_1);
