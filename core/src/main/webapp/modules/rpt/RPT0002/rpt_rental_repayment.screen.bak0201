            function summaryRenderer_detail(datas, name) {
                //debugger;
                var sum = 0;
                var sum2 = 0;
                var sum3 = 0;
                for (var i = 0;i < datas.length;i++) {
                    record = datas[i];
                    if (name == "due_amount") {
                        var t_1 = record.get("due_amount");
                        var t_2 = parseFloat(t_1);
                        if (!isNaN(t_2)) {
                            sum += t_2;
                        }
                    }
                    if (name == "principal") {
                        var d_1 = record.get("principal");
                        var d_2 = parseFloat(d_1);
                        if (!isNaN(d_2)) {
                            sum2 += d_2;
                        }
                    }
            
                    if (name == "interest") {
                        var p_1 = record.get("interest");
                        var p_2 = parseFloat(p_1);
                        if (!isNaN(p_2)) {
                            sum3 += p_2;
                        }
                    }
                }
            
                if (name == "due_date") {
                    return '<div align="right"><font color="red">合计:</font></div>';
                }
                if (name == "due_amount") {
                    return '<font color="red">' + Aurora.formatNumber(sum, 2) + '</font>';
                }
                if (name == "principal") {
                    return '<font color="red">' + Aurora.formatNumber(sum2, 2) + '</font>';
                }
                if (name == "interest") {
                    return '<font color="red">' + Aurora.formatNumber(sum3, 2) + '</font>';
                }
            }
            
            function render_info(value, record, name) {
                if (name == 'rental_repayment_detail_info') {
                    return '<a href="javascript:rental_repayment_detail(' + record.get('unit_id') + ')">明细</a>';
                }
