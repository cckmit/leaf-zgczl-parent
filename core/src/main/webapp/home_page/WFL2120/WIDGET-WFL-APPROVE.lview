            }/*表格行*/
            .widget_wfl_approve .hls-pagging-grid .hls-table tbody tr:hover{
                background: rgba(90,127,255,0.08);
            }
            .widget_wfl_approve .hls-pagging-grid .hls-table .workflow_info_box{
                min-width:200px;
